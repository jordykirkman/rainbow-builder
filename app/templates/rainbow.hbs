<div class="artboard">
	<svg width="800" height="600">
		<g class="rainbow">
			<circle cx="400" cy="600" r="400" {{bind-attr fill="color1"}}/>
			<circle cx="400" cy="600" r="350" {{bind-attr fill="color2"}}/>
			<circle cx="400" cy="600" r="300" {{bind-attr fill="color3"}}/>
			<circle cx="400" cy="600" r="250" fill="white"/>
		</g>
	</svg>
</div>
<div class="controlls">
	{{input type="range"}}
	{{color}}
	{{input type="color" valueBinding="color1"}}
	{{input type="color" valueBinding="color2"}}
	{{input type="color" valueBinding="color3"}}
</div>